                cstr = testocx.GetImgNameByIdx(i);
                if (cstr!="")
                {
                    var opt = document.createElement("OPTION");
                    sel.options.add(opt);
                    opt.innerText = cstr;
                    opt.value = i;
                }
            }
        }

                                                               totalnum =totalnum+(Convert.ToInt32(dt.Rows[i]["AcceptQua1"].ToString())+Convert.ToInt32(dt.Rows[i]["AcceptQua1"].ToString())+Convert.ToInt32(dt.Rows[i]["AcceptQua1"].ToString()));
                                                               dealnum1 = dealnum1 + Convert.ToInt32(dealqua1);
